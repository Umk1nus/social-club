{
  "compilerOptions": {
      "target": "esnext",
      "module": "esnext",
      "moduleResolution": "node",
      "strict": true,
      "jsx": "preserve",
      "sourceMap": true,
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "lib": ["esnext", "dom"],
      "types": ["node", "stripe-v3", "vite/client"],
      "baseUrl": ".",
      "paths": {
          "@/*": ["resources/js/*"],
      }
  },
  "include": ["resources/**/*", "env.d.ts"],
  "exclude": ["public/**/*", "node_modules", "vendor"]
}